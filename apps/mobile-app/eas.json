{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "http://localhost:3001/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://api-staging.smartequiz.com"
      }
    },
    "production": {
      "env": {
        "API_URL": "https://api.smartequiz.com"
      }
    },
    "tenant-production": {
      "extends": "production",
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}

{
  "name": "smart-equiz-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "nest start --watch",
    "start": "node dist/main.js",
    "build": "nest build",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "seed": "node prisma/seed.js",
    "deploy:railway": "railway up",
    "deploy:render": "render deploy",
    "healthcheck": "curl -f http://localhost:3000/api/health || exit 1",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload --org smart-equiz --project api dist"
  },
  "dependencies": {
    "@nestjs/config": "^3.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/client": "^5.0.0",
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.7",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "stripe": "^14.0.0",
    "@stripe/stripe-js": "^2.4.0",
    "@sendgrid/mail": "^8.1.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.12",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@types/express": "^4.17.17",
    "@types/node": "^18.19.130",
    "prisma": "^5.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  }
}
